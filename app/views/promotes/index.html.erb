<p id="notice"><%= notice %></p>

<h1>Promotes</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>From class</th>
      <th>To class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @promotes.each do |promote| %>
      <tr>
        <td><%= promote.student_id %></td>
        <td><%= promote.from_class_id %></td>
        <td><%= promote.to_class_id %></td>
        <td><%= link_to 'Show', promote %></td>
        <td><%= link_to 'Edit', edit_promote_path(promote) %></td>
        <td><%= link_to 'Destroy', promote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Promote', new_promote_path %>
